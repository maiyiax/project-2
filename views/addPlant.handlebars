<div class="plants list-group">
  

<div class="post card">
  <div class="card-header">
    <h2>Add Plant</h2>
  </div>

 <ul>
{{#each plantdata}}
    <li>{{.}}</li>
{{/each}}
</ul> 

  <form id="new-post-form" class="card-body">
    <label class="form-label" for="post-title">Plant</label>
    <input type="text" name="post-title" class="form-input" />

    <label class="form-label" for="post-body">About Plant</label>
    <textarea name="post-body" class="form-input"></textarea>

    <button type="submit" class="btn">Create</button>
  </form>
</div>

<script src="/javascript/add-plant.js"></script>
